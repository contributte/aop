{
  "name": "contributte/aop",
  "type": "library",
  "description": "AOP for Nette Framework Dependency Injection Container",
  "keywords": [
    "nette",
    "contributte",
    "aop",
    "di",
    "dic",
    "aspect"
  ],
  "homepage": "https://github.com/contributte/aop",
  "license": "MIT",
  "authors": [
    {
      "name": "Dalibor Korpar",
      "email": "dalibor.korpar@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "nette/di": "^3.0.3",
    "nette/php-generator": "^3.5|^4.0",
    "nette/tokenizer": "~3.0",
    "symfony/property-access": "^4.4|~5.0|~6.0"
  },
  "require-dev": {
    "nette/application": "^3.1",
    "nette/bootstrap": "~3.1",
    "nette/caching": "~3.1",
    "nette/component-model": "~3.0",
    "nette/database": "~3.1",
    "nette/finder": "~2.5",
    "nette/forms": "~3.1",
    "nette/http": "~3.1",
    "nette/mail": "~3.1",
    "nette/neon": "~3.1",
    "nette/robot-loader": "^3.4",
    "nette/safe-stream": "~2.4",
    "nette/security": "~3.0",
    "nette/utils": "^3.2",
    "latte/latte": "~2.10|^3.0",
    "tracy/tracy": "~2.8",
    "doctrine/collections": "~1.6",
    "contributte/qa": "^0.4.0",
    "contributte/phpstan": "^0.2.0",
    "contributte/phpunit": "^0.2.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Aop\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
